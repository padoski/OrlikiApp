{
  "auth": {
    "Orliki": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false
    }
  },
  "api": {
    "Orliki": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS",
            "userPoolConfig": {
              "userPoolId": "authOrliki"
            }
          },
          "additionalAuthenticationProviders": [
            {
              "authenticationType": "API_KEY",
              "apiKeyConfig": {
                "apiKeyExpirationDays": 30,
                "description": "api key description"
              }
            },
            {
              "authenticationType": "AWS_IAM"
            }
          ]
        }
      }
    },
    "StripeWebHook": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "StripeWebHook",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "function": {
    "CreatePaymentIntent": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    },
    "StripeWebHook": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "storage": {
    "Orlik": {
      "service": "S3",
      "providerPlugin": "awscloudformation"
    }
  }
}